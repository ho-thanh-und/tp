@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model as ModelPackage <<Rectangle>>{
Class "<<interface>>\nReadOnlyAddressBook" as ReadOnlyAddressBook
Class "<<interface>>\nReadOnlyUserPrefs" as ReadOnlyUserPrefs
Class "<<interface>>\nReadOnlyScheduleBoard" as ReadOnlyScheduleBoard
Class "<<interface>>\nModel" as Model
Class AddressBook
Class ScheduleBoard
Class ModelManager
Class UserPrefs

Class UniquePersonList
Class Person
Class Address
Class Email
Class Name
Class Phone
Class Tag

Class UniqueScheduleList
Class Schedule
Class LocalDate
Class LocalTime

Class I #FFFFFF
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

AddressBook .up.|> ReadOnlyAddressBook
ScheduleBoard .up.|> ReadOnlyScheduleBoard

ModelManager .up...|> Model
Model ..> ReadOnlyUserPrefs
Model ..> ReadOnlyAddressBook
Model ...> ReadOnlyScheduleBoard
ModelManager ---> "1" AddressBook
ModelManager ---> "1" UserPrefs
ModelManager ---> "1" ScheduleBoard
UserPrefs .up.|> ReadOnlyUserPrefs

AddressBook *--> "1" UniquePersonList
UniquePersonList --> "~* all" Person
Person *--> Name
Person *--> Phone
Person *--> Email
Person *--> Address
Person *--> "*" Tag

ScheduleBoard *--> "1" UniqueScheduleList
UniqueScheduleList --> "~* all" Schedule
Schedule *--> LocalDate
Schedule *--> Name
Schedule *--> Email
Schedule *--> LocalTime

Person -[hidden]up--> I
UniquePersonList -[hidden]right-> I

Name -[hidden]right-> Phone
Phone -[hidden]right-> Address
Address -[hidden]right-> Email

ModelManager --> "~* filtered" Person
@enduml
